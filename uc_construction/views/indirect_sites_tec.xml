<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sites_tec_tree" model="ir.ui.view">
            <field name="name">sites.tec.tree</field>
            <field name="model">tender.indirect.sites.technician</field>
            <field name="arch" type="xml">
                <list string="">
                    <field name="technician_number"/>
                    <field name="duration"/>
                    <field name="month_cost"/>
                    <field name="currency_id"/>
                    <field name="company_currency" invisible="1"/>
                    <field name="total_cost"/>
                    <field name="total_cost_egy"/>
                </list>
            </field>
        </record>
        <record id="sites_tec_form" model="ir.ui.view">
            <field name="name">sites.tec.form</field>
            <field name="model">tender.indirect.sites.technician</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="technician_number"/>
                            <field name="duration"/>
                            <field name="month_cost"/>
                            <field name="currency_id"/>
                            <field name="company_currency" invisible="1"/>
                            <field name="total_cost"/>
                            <field name="total_cost_egy"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_sites_tec123" model="ir.actions.act_window">
            <field name="name">Indirect Sites Technician</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tender.indirect.sites.technician</field>
            <field name="view_mode">list,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
                <p>
                    <!-- More details about what a user can do with this object will be OK -->
                </p>
            </field>
        </record>


    </data>
</odoo>