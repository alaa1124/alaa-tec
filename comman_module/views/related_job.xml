<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="related_job_view_tree" model="ir.ui.view">
        <field name="name">related_job_view_tree</field>
        <field name="model">project.related.job</field>
        <field name="arch" type="xml">
            <tree string="item_tree">
                <field name="name"/>
                <field name="code"/>

            </tree>
        </field>
    </record>
    <record id="related_job_view_form" model="ir.ui.view">
        <field name="name">related_job_view_form</field>
        <field name="model">project.related.job</field>
        <field name="arch" type="xml">
            <form string="item_tree">
                <group>
                    <field name="name"/>
                    <field name="code"/>


                </group>

            </form>
        </field>
    </record>
</odoo>