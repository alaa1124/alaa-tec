<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <record id="item_view_tree" model="ir.ui.view">
       <field name="name">item_view_tree</field>
       <field name="model">project.item</field>
       <field name="arch" type="xml">
           <tree string="item_tree">
               <field name="name"/>
               <field name="related_job_id"/>
               <field name="uom_id"/>
           </tree>
       </field>
   </record>
   <record id="item_view_form" model="ir.ui.view">
       <field name="name">item_view_tree</field>
       <field name="model">project.item</field>
       <field name="arch" type="xml">
           <form string="item_tree">
               <group>
                    <field name="name"/>
               <field name="related_job_id"/>
               <field name="uom_id"/>

               </group>

           </form>
       </field>
   </record>
</odoo>